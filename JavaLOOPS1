package hackerrank;
import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a = sc.nextInt();
        for (int i = 1; i <= 10; i++) {
            int hasil = a * i;
            System.out.println(a + " x " + i + " = " + hasil);
        }
    }
}
